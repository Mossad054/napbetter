{
  "expo": {
    "name": "Daylio Clone React Native App",
    "slug": "daylio-clone-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.daylio-clone-react-native",
      "entitlements": {
        "com": {
          "apple": {
            "security": {
              "application-groups": [
                "group.com.myapp"
              ]
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "app.rork.daylio-clone-react-native",
      "permissions": [
        "android.permission.VIBRATE"
      ]
    },
    "web": {
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "enabled": false,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}